<UserControl x:Class="SilverAudioPlayer.WPF.TimedProgressBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SilverAudioPlayer.WPF"
             mc:Ignorable="d" d:DesignWidth="800" Height="54">
    <Grid>
        <local:LameProgressBar x:Name="ProgressBar" Margin="0,0,0,34" />
        <Label x:Name="LeftLabel" Content="00:00:00" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top" Height="17" FontSize="7" />
        <Label x:Name="RightLabel" Content="00:00:00" HorizontalAlignment="Right" Margin="0,21,10,0" VerticalAlignment="Top" Height="17" FontSize="7" />
    </Grid>
</UserControl>